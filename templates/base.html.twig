<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}Bid - {% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-md bg-light py-1 d-flex test">
            <div class="container">
                <a class="navbar-brand" href="{{ path('index') }}">Enchere.fr</a>

                <ul class="navbar m-0 p-0 list-unstyled navbar-search" id="navbarNav">
                    <li class="nav-item">
                        <input type="text" class="form-control form-control-sm" placeholder="Rechercher un objet, un lot, etc">
                    </li>
                    <li class="nav-item">
                        <select class="form-control-sm w-100">
                            <option selected>Choisir une catégorie</option>
                            <option value="1">Valeur</option>
                            <option value="2">Valeur</option>
                            <option value="3">Valeur</option>
                            <option value="4">Valeur</option>
                        </select>
                    </li>
                    <li class="nav-item">
                        <a class="btn btn-info  btn-sm w-100">Rechercher</a>
                    </li>
                </ul>

                <ul class="d-flex m-0 list-unstyled header-side">
                    {% if app.user %}
                        <li class="pr-2 position-relative">
                            <site-notifications></site-notifications>
                        </li>
                        <li class="pr-2">
                            <a href="">
                                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-person-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                                </svg>
                            </a>
                        </li>
                    {% else %}
                        <li class="pr-2">
                            <a href="{{ path('login') }}">
                                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-person-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                                </svg>
                            </a>
                        </li>
                    {% endif %}
                    <button class="burger p-0 border-0 bg-transparent" type="button">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-list" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd" d="M2.5 11.5A.5.5 0 0 1 3 11h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4A.5.5 0 0 1 3 7h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4A.5.5 0 0 1 3 3h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
                        </svg>
                    </button>
                </ul>



            </div>
